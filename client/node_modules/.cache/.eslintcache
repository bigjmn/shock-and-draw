 'react'\nimport socket from '../../context/socket.js'\n\nimport Timer from '../Timer/Timer.js'\nimport classes from './RoundClock.module.css'\nconst RoundClock = ({maxTime}) => {\n\n  const [startTime, setStartTime] = useState(0)\n\n  const [secs, setSecs] = useState(0)\n\n\n  const secSetter = (time) => {\n    setSecs(time)\n    if (time <= 0){\n      setStartTime(0)\n      socket.emit('roundOver')\n    }\n  }\n\n  useEffect(() => {\n    socket.on('startRound', (data) => {\n      setStartTime(data.timeStamp)\n    })\n    return () => {\n      socket.off('startRound')\n    }\n  })\n\n  return(\n    <div>\n      <h1>{secs}</h1>\n      <Timer secSetter={secSetter} maxTime={maxTime} startTime={startTime}/>\n\n    </div>\n  )\n}\n\nexport default RoundClock\n","/Users/jessenicholas/shock-draw-final/client/src/components/Timer/Timer.js",["200","201","202"],"import {useEffect, useState } from 'react'\nconst Timer = ({maxTime, startTime, secSetter}) => {\n\n\n\n\n\n\n\n\n  useEffect(() => {\n    let int = null\n    if (startTime == 0){\n      return\n    }\n    int = setInterval(() => {\n\n      secSetter(maxTime-(Math.round((Date.now() - startTime)/1000)))\n\n    }, 1000)\n    return function cleanup(){\n      console.log('clearing')\n      clearInterval(int)\n\n    }\n\n  }, [startTime])\n\n  return (\n    <div>\n\n    </div>\n  )\n\n}\n\nexport default Timer\n","/Users/jessenicholas/shock-draw-final/client/src/components/TrashButton/TrashButton.js",[],"/Users/jessenicholas/shock-draw-final/client/src/components/BonusClock/BonusClock.js",[],"/Users/jessenicholas/shock-draw-final/client/src/components/AttackZone/AttackZone.js",["203"],"import socket from '../../context/socket.js'\nimport { useEffect, useState } from 'react'\n\nimport AttackCard from '../AttackCard/AttackCard.js'\nimport attacklist from './attacklist.js'\n\nimport classes from './AttackZone.module.css'\n\nconst AttackZone = ({maxTime}) => {\n\n  const [attacks, setAttacks] = useState([])\n\n  const filterOut = (timestamp) => {\n    socket.emit('clearAttack', {timestamp:timestamp})\n    setAttacks(attacks => attacks.filter(x => (x.timestamp != timestamp)))\n  }\n\n\n\n  useEffect(() => {\n    socket.on('takeAttack', (data) => {\n      let attackPack = {\n        timestamp: data.timestamp,\n        color: data.color,\n        type: attacklist[data.type]\n      }\n      setAttacks((oldAttackList) => [...oldAttackList, attackPack])\n    })\n    return () => {\n      socket.off('takeAttack')\n    }\n  })\n\n  return (\n    <div className={classes.attackContainer}>\n      {attacks.map((card) => (\n        <div key={card.timestamp}>\n          <AttackCard\n            type={card.type}\n            timestamp={card.timestamp}\n            color={card.color}\n            maxTime={maxTime}\n            filterOut={filterOut}/>\n        </div>\n      ))}\n    </div>\n  )\n}\n\nexport default AttackZone\n","/Users/jessenicholas/shock-draw-final/client/src/components/AttackCard/AttackCard.js",["204","205"],"import {useState, useEffect} from 'react'\nimport socket from '../../context/socket.js'\n\nimport classes from './AttackCard.module.css'\n\nconst AttackCard = ({type, color, timestamp, maxTime, filterOut}) => {\n\n  const [secs, setSecs] = useState(maxTime)\n\n  useEffect(() => {\n    let int = null\n    int = setInterval(() => {\n      let timeleft = maxTime-(Math.round((Date.now() - timestamp)/1000))\n\n\n      if (timeleft <= 0){\n        console.log(timeleft)\n        filterOut(timestamp)\n      }\n      setSecs(timeleft)\n    }, 1000)\n    return () => {\n      clearInterval(int)\n    }\n\n  }, [])\n\n  return (\n    <div style={{backgroundColor:color}} className={classes.cardContainer}>\n      <h1>{type.title}</h1>\n      <h2>{type.explainer}</h2>\n      <div className={classes.timerspot}>\n        {secs}\n      </div>\n      <h3>{type.flavor}</h3>\n\n    </div>\n  )\n}\n\nexport default AttackCard\n","/Users/jessenicholas/shock-draw-final/client/src/components/AttackZone/attacklist.js",[],"/Users/jessenicholas/shock-draw-final/client/src/components/PeepCanvas/PeepCanvas.js",["206","207"],"import React, { useState, useEffect } from \"react\";\n\n\n\nfunction PeepCanvas() {\n  const canvasRef = React.useRef(null);\n  const parentRef = React.useRef(null);\n  const [ctx, setCtx] = useState({});\n  const [canvasOffset, setCanvasOffset] = useState({ x: 0, y: 0 });\n  const [peeping, setPeeping] = useState(false);\n\n\n\n  // const [drawPack, setDrawPack] = useState([])\n\n\n\n\n  // const addPack = (payload) => {\n  //   setDrawPack((oldPack) =>  [...oldPack, payload])\n  //   console.log(drawPack)\n  // }\n  function throttled(delay, fn) {\n  let lastCall = 0;\n  return function (...args) {\n    const now = (new Date).getTime();\n    if (now - lastCall < delay) {\n      return;\n    }\n    lastCall = now;\n    return fn(...args);\n  }\n}\n\n\n\n\n\n\n\n\n\n\n  useEffect(() => {\n    let canv = canvasRef.current;\n    canv.width = parentRef.current.offsetWidth;\n    canv.height = parentRef.current.offsetHeight;\n\n    let canvCtx = canv.getContext(\"2d\");\n    canvCtx.fillStyle = \"black\";\n\n\n    setCtx(canvCtx);\n\n    let offset = canv.getBoundingClientRect();\n    setCanvasOffset({ x: parseInt(offset.left), y: parseInt(offset.top) });\n    canvCtx.fillRect(0,0,500,500)\n\n  }, [ctx]);\n\n\n\n  function handleMouseDown(e) {\n    setPeeping(true);\n    let mousex = e.clientX - canvasOffset.x;\n    let mousey = e.clientY - canvasOffset.y;\n    ctx.clearRect(mousex-90,mousey-90,180,180)\n  }\n  function handleMouseUp() {\n    setPeeping(false);\n    ctx.fillRect(0,0,500,500)\n  }\n  function handleMouseOut() {\n    setPeeping(false);\n    ctx.fillRect(0,0,500,500)\n  }\n\n  const handleMouseMove = (e) => {\n    let mousex = e.clientX - canvasOffset.x;\n    let mousey = e.clientY - canvasOffset.y;\n    if (peeping) {\n      ctx.fillRect(0,0,500,500)\n      ctx.clearRect(mousex-90,mousey-90,180,180)\n\n\n    }\n\n  }\n\n  const throttledhandleMouseMove = throttled(50, handleMouseMove)\n\n\n\n  return (\n\n\n\n    <div className=\"PeepCanvas\" ref={parentRef}>\n\n\n\n\n\n      <canvas\n        ref={canvasRef}\n        onMouseDown={handleMouseDown}\n        onMouseUp={handleMouseUp}\n        onMouseOut={handleMouseOut}\n        onMouseMove={handleMouseMove}\n        height=\"500\" width=\"500\"\n\n\n        style={{border:\"1px solid\" }}\n\n      />\n\n    </div>\n\n  );\n\n}\n\nexport default PeepCanvas;\n","/Users/jessenicholas/shock-draw-final/client/src/layouts/Recap/Recap.js",["208"],"import {useEffect, useState} from 'react'\nimport socket from '../../context/socket.js'\n\nimport classes from './Recap.module.css'\n\nconst Recap = (props) => {\n\n  useEffect(() => {\n    setTimeout(() => {socket.emit('getPreview')}, 5000)\n  }, [])\n\n\n  return (\n    <div className={classes.recapContainer}>\n      <h1>End of Round {props.round}</h1>\n    <div className={classes.pointsRecapContainer}>\n      <div className={classes.totalpointsHolder} >\n        <h1 style={{color:\"#ff3b2d\"}}>{props.redDrawer}</h1>\n\n        <h1 style={{color:\"#ff3b2d\"}}>{props.totalRedPoints}</h1>\n        <h1 style={{color:\"#ff3b2d\"}}>+ {props.redPoints}</h1>\n        <h1 style={{backgroundColor:\"#ff3b2d\"}}>{props.redPoints + props.totalRedPoints}</h1>\n      </div>\n      <div className={classes.totalpointsHolder} >\n        <h1 style={{color:\"#2df1ff\"}}>{props.blueDrawer}</h1>\n\n        <h1 style={{color:\"#2df1ff\"}}>{props.totalBluePoints}</h1>\n        <h1 style={{color:\"#2df1ff\"}}>+ {props.bluePoints}</h1>\n        <h1 style={{backgroundColor:\"#2df1ff\"}}>{props.bluePoints + props.totalBluePoints}</h1>\n      </div>\n    </div>\n    </div>\n  )\n}\n\nexport default Recap\n","/Users/jessenicholas/shock-draw-final/client/src/components/PassButton/PassButton.js",[],"/Users/jessenicholas/shock-draw-final/client/src/components/ScoreCard/ScoreCard.js",["209","210","211"],"import { useState, useEffect } from 'react'\nimport socket from '../../context/socket.js'\nimport classes from './ScoreCard.module.css'\n\nconst ScoreCard = (props) => {\n\n\n  useEffect(() => {\n    socket.on('takePoints', (data) => {\n      if (data.color == 'red'){\n        props.setRedPoints(x => x+1)\n      }\n      if (data.color == 'blue'){\n        props.setBluePoints(x => x+1)\n      }\n    })\n    return () => {\n      socket.off('takePoints')\n    }\n  })\n\n  return (\n    <div className={classes.pointsContainer}>\n      <div className={classes.teampointsContainer}\n        style={{backgroundColor:\"#ff3b2d\"}}><h1>{props.redPoints}</h1></div>\n      <div className={classes.teampointsContainer}\n        style={{backgroundColor:\"#2df1ff\"}}><h1>{props.bluePoints}</h1></div>\n    </div>\n  )\n\n}\n\nexport default ScoreCard\n","/Users/jessenicholas/shock-draw-final/client/src/components/PlayerTags/PlayerTags.js",["212","213"],"import {useState, useEffect} from 'react'\nimport classes from './PlayerTags.module.css'\n\nconst PlayerTags = ({teamTags, oppTags, teamColor, oppColor}) => {\n\n  return (\n    <div className={classes.playerTagWrapper}>\n      <div className={classes.tagListWrapper}>\n        <h3>My Team</h3>\n        {teamTags.map((tag, i) => (\n          <div key={i} className={classes.tagStyle} style={{backgroundColor:teamColor}}>\n            {tag}\n          </div>\n        ))}\n      </div>\n      <div className={classes.tagListWrapper}>\n        <h3>Opponents</h3>\n        {oppTags.map((tag, i) => (\n          <div key={i} className={classes.tagStyle} style={{backgroundColor:oppColor}}>\n            {tag}\n          </div>\n        ))}\n      </div>\n    </div>\n  )\n}\n\nexport default PlayerTags\n","/Users/jessenicholas/shock-draw-final/client/src/layouts/EndGame/EndGame.js",["214","215"],"import socket from '../../context/socket.js'\nimport {useEffect, useState} from 'react'\n\nimport classes from './EndGame.module.css'\n\nconst EndGame = ({totalRedPoints, totalBluePoints}) => {\n\n  useEffect(() => {\n    setTimeout(() => {socket.emit('backtoWaitRoom')}, 8000)\n  }, [])\n\n  return (\n    <div className={classes.finalScoreContainer}>\n      {totalRedPoints > totalBluePoints &&\n      <h1 style={{color:\"#ff3b2d\"}}>RED TEAM WINS!</h1>\n      }\n      {totalBluePoints > totalRedPoints &&\n      <h1 style={{color:\"#2df1ff\"}}>BLUE TEAM WINS!</h1>\n      }\n      {totalBluePoints == totalRedPoints &&\n      <div>\n        <h1>It's TIE!</h1>\n        <p>A tie? Seriously?</p>\n      </div>\n      }\n      <div className={classes.scoresBar}>\n        <div className={classes.scoreContainer} style={{backgroundColor:\"#ff3b2d\"}}>\n          <h2>{totalRedPoints}</h2>\n        </div>\n        <div className={classes.scoreContainer} style={{backgroundColor:\"#2df1ff\"}}>\n          <h2>{totalBluePoints}</h2>\n        </div>\n      </div>\n\n\n    </div>\n  )\n}\n\nexport default EndGame\n","/Users/jessenicholas/shock-draw-final/client/src/components/ColorPanel/NewColorPanel.js",[],"/Users/jessenicholas/shock-draw-final/client/src/components/AudioPlayer/AudioPlayer.js",["216","217"],"import React, { useState, useEffect } from \"react\";\nimport socket from '../../context/socket.js'\nimport sound from './correct.mp3'\nconst useAudio = url => {\n  const [audio] = useState(new Audio(url));\n  const [playing, setPlaying] = useState(false);\n\n  const toggle = () => setPlaying(!playing);\n\n  useEffect(() => {\n      playing ? audio.play() : audio.pause();\n    },\n    [playing]\n  );\n\n  useEffect(() => {\n    audio.addEventListener('ended', () => setPlaying(false));\n    return () => {\n      audio.removeEventListener('ended', () => setPlaying(false));\n    };\n  }, []);\n\n  return [playing, toggle];\n};\n\nconst Player = () => {\n  const [playing, toggle] = useAudio(sound);\n  useEffect(() => {\n    socket.on('correct', () => document.getElementById('clickplayer').click())\n    return () => socket.off('correct')\n  })\n\n  return (\n    <div style